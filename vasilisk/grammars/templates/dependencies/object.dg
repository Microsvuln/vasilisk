%%% JSObjects

%section% := value

any_object :=
    !obj!
    +value:valtype+


statement :=
    !obj![+common:decimal_number+] = +value:valtype+

name_value_pair :=
    "+common:text+": +value:valtype+

objtype :=
    {%repeat%(+name_value_pair+, ",")}
    new Proxy({%repeat%(+name_value_pair+, ",")}, +proxy:proxyhandler+)

%section% := variable

obj :=
    var @obj@; try { @obj@ = +objtype+; } catch (e) {}
